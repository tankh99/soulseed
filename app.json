{
  "expo": {
    "name": "soulseed",
    "slug": "soulseed",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/base_form.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.soulseed"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/base_form.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/base_form.png",
          "color": "#FFFFFF"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
